{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/(.*)",
        "destination": "https://www.shopsportscontinent.com/$1",
        "has": [
          {
            "type": "host",
            "value": "shopsportscontinent.com"
          }
        ],
        "permanent": true
      }
    ]
  }